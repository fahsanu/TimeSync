{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-inapp.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1",
      "infoPlist": {
      "UIBackgroundModes": ["audio"]
      },
      "bundleIdentifier": "com.fakeapp"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fakeapp",
      "permissions": ["WAKE_LOCK"],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo-timesync.png",
          "color": "#000000",
          "sounds": "./assets/sounds/alarm-sound.mp4"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "expo-notifications": {
      "icon": "./assets/images/favicon.png",
      "color": "#000000"
    }
  }
}